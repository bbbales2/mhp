@article{threeDM,
  author={P. G. Callahan and M. De Graef},
  title={Precipitate shape fitting and reconstruction by means of {3D} {Zernike} functions},
  journal={Modelling and Simulation in Materials Science and Engineering},
  volume={20},
  number={1},
  pages={015003},
  url={http://stacks.iop.org/0965-0393/20/i=1/a=015003},
  year={2012},
  abstract={3D Zernike functions are defined and used for the reconstruction of precipitate shapes. These functions are orthogonal over the unit ball and allow for an arbitrary shape, scaled to fit inside an embedding sphere, to be decomposed into 3D harmonics. Explicit expressions are given for the general Zernike moments, correcting typographical errors in the literature. Explicit expressions of the Zernike moments for the ellipsoid and the cube are given. The 3D Zernike functions and moments are applied to the reconstruction of γ' precipitate shapes in two Ni-based superalloys, one with nearly cuboidal precipitate shapes, and one with more complex dendritic shapes.}
}
	
